{
    "name": "tauri-plugin-torch-api",
    "version": "0.1.0",
    "author": "SoSweetHam <ham@kodski.com>",
    "description": "A simple flash/torch control plugin for Tauri applications.",
    "license": "MIT",
    "type": "module",
    "types": "./dist-js/index.d.ts",
    "main": "./dist-js/index.cjs",
    "module": "./dist-js/index.js",
    "exports": {
        "types": "./dist-js/index.d.ts",
        "import": "./dist-js/index.js",
        "require": "./dist-js/index.cjs"
    },
    "files": [
        "dist-js",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sosweetham/tauri-plugin-torch"
    },
    "keywords": [
        "tauri",
        "plugin",
        "torch",
        "flash",
        "android",
        "ios"
    ],
    "scripts": {
        "build": "rollup -c",
        "prepublishOnly": "pnpm build",
        "pretest": "pnpm build",
        "lint": "pnpm biome lint --write ./guest-js",
        "lint-all": "pnpm -F tauri-* lint",
        "check-lint-all": "pnpm -F tauri-* check-lint",
        "check-lint": "pnpm biome lint ./guest-js",
        "format": "pnpm biome format --write ./guest-js",
        "check-format": "pnpm biome format ./guest-js",
        "format-all": "pnpm -F tauri-* format",
        "check-format-all": "pnpm -F tauri-* check-format",
        "check": "pnpm biome check --write ./guest-js",
        "check-all": "pnpm -F tauri-* check",
        "check-types": "tsc --noEmit",
        "check-types-all": "pnpm -F tauri-* check-types"
    },
    "dependencies": {
        "@tauri-apps/api": ">=2.0.0-beta.6"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "rollup": "^4.9.6",
        "tslib": "^2.6.2",
        "typescript": "^5.3.3"
    },
    "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
